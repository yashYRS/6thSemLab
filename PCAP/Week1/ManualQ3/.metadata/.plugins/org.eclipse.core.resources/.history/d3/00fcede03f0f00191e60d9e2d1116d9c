#include<mpi.h>
#include<stdio.h>
int main(int argc, char *argv[]) {
	int rank, size ;
	int a = 10 , b = 4, res_add, res_sub, res_mul;
	float res_div ;
	MPI_Init(&argc,&argv);
	MPI_Comm_rank(MPI_COMM_WORLD,&rank) ;
	MPI_Comm_size(MPI_COMM_WORLD,&size) ;
	if (rank == 0)
		printf("%d",a + b) ;
	else if (rank == 1)
		printf("%d",a - b );
	else if (rank == 2)
		printf("%d",a*b );
	else
		printf("%f",(float)a/(float)b) ;
	MPI_Finalize();
	return 0 ;
}
